[sub_resource type="Shader" id=14]
code = "shader_type canvas_item;

uniform float starVisibility : hint_range(0.0, 1.0) = 0.0;
uniform float starExistanceCutoff : hint_range(0.0, 1.0) = 0.002;
uniform float brightnessVariability : hint_range(0.0, 1.0) = 0.25;

float random(vec2 uv) {
    return fract(sin(dot(uv.xy,
        vec2(12.9898,78.233))) * 43758.5453123);
}

void fragment() {
	vec4 color = texture(TEXTURE, UV);
	float randomValue = random(UV);
	float starBrightness = 0.0;
	if (randomValue < starExistanceCutoff) {
		float variation = brightnessVariability * cos(TIME * 11.0 * UV.x) * sin(TIME * 13.0 * UV.y);
		starBrightness = randomValue * (1.0 + variation) / starExistanceCutoff;
		color.rgb = mix(color.rgb, vec3(1.0), starBrightness * starVisibility);
	}
	COLOR = color;
}"

